{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["google"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "block-spacing": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off",
    "padded-blocks": "off",
    "require-jsdoc": "off"
  }
}
